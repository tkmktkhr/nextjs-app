{
  "appenders": {
    "stdout": {
      "type": "stdout",
      "layout": {
        "type": "pattern",
        "pattern": "[%d{yyyy-MM-dd hh:mm:ss:SSS}] {lwt-%p}:%m"
      }
    }
  },
  "categories": {
    "default": { "appenders": ["stdout"], "level": "warn" },
    "debug": { "appenders": ["stdout"], "level": "debug" },
    "info": { "appenders": ["stdout"], "level": "info" }
  }
}
